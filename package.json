{
  "name": "health-command-center",
  "version": "1.0.0",
  "private": true,
  "description": "Macronutrient Tracker App",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "synth": "pnpm --filter @health-command-center/cdk synth",
    "deploy:cert": "pnpm --filter @health-command-center/cdk deploy:cert",
    "deploy:domain": "pnpm --filter @health-command-center/cdk deploy:domain",
    "deploy:auth": "pnpm --filter @health-command-center/cdk deploy:auth",
    "deploy:api": "pnpm --filter @health-command-center/cdk deploy:api",
    "deploy:frontend": "pnpm --filter @health-command-center/cdk deploy:frontend",
    "deploy:cloudfront": "pnpm --filter @health-command-center/cdk deploy:cloudfront",
    "deploy:all": "pnpm --filter @health-command-center/cdk deploy:all",
    "cdk": "pnpm --filter @health-command-center/cdk cdk",
    "layer:install": "pnpm --filter @health-command-center/cdk layer:install",
    "layer:update": "pnpm --filter @health-command-center/cdk layer:update",
    "predeploy:api": "pnpm run layer:install"
  },
  "keywords": [
    "aws",
    "cdk",
    "react",
    "typescript",
    "node",
    "serverless"
  ],
  "author": "Your Name",
  "license": "ISC",
  "devDependencies": {
    "typescript": "~5.3.3",
    "cross-env": "^7.0.3"
  },
  "packageManager": "pnpm@8.6.0"
}
