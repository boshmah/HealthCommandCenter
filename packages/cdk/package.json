  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.481.0",
    "@aws-sdk/lib-dynamodb": "^3.481.0",
    "@health-command-center/types": "workspace:*",
    "aws-cdk-lib": "^2.118.0",
